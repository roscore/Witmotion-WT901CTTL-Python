<launch>

    <node pkg="wt901cttl_ros" type="WT901.py" name="WT901_0" output="screen">

        <param name="serial_port_name"      value="/dev/ttyUSB0" /> <!-- Name of serial port -->
        <param name="serial_baud_rate"      value="9600" />  <!--Baudrate of serial port-->
        <param name="sensor_name"           value="center" />
    </node>



    <node pkg="wt901cttl_ros" type="WT901.py" name="WT901_1" output="screen">

        <param name="serial_port_name"      value="/dev/ttyUSB1" /> <!-- Name of serial port -->
        <param name="serial_baud_rate"      value="9600" />  <!--Baudrate of serial port-->
        <param name="senser_name"            value="front_left" />
    </node>



    <node pkg="wt901cttl_ros" type="WT901.py" name="WT901_2" output="screen">

        <param name="serial_port_name"      value="/dev/ttyUSB2" /> <!-- Name of serial port -->
        <param name="serial_baud_rate"      value="9600" />  <!--Baudrate of serial port-->
        <param name="senser_name"            value="front_right" />
    </node>



    <node pkg="wt901cttl_ros" type="WT901.py" name="WT901_3" output="screen">

        <param name="serial_port_name"      value="/dev/ttyUSB3" /> <!-- Name of serial port -->
        <param name="serial_baud_rate"      value="9600" />  <!--Baudrate of serial port-->
        <param name="senser_name"            value="back_left" />
    </node>



    <node pkg="wt901cttl_ros" type="WT901.py" name="WT901_4" output="screen">

        <param name="serial_port_name"      value="/dev/ttyUSB4" /> <!-- Name of serial port -->
        <param name="serial_baud_rate"      value="9600" />  <!--Baudrate of serial port-->
        <param name="senser_name"            value="back_right" />
    </node>


</launch>
